{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
    "no-console": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "consistent-return": 0,
    "quotes": [
      "error",
      "single"
    ],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always"
      }
   ]
  }
}